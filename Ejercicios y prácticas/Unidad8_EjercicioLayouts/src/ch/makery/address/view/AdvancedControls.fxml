<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Shufle">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../images/suffle.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </MenuItem>
                  <MenuItem mnemonicParsing="false" text="Clear">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem mnemonicParsing="false" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
                  <Menu mnemonicParsing="false" text="Picture Effect">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Sepia Tone" />
                        <MenuItem mnemonicParsing="false" text="Glow" />
                        <MenuItem mnemonicParsing="false" text="Shadow" />
                    </items>
                  </Menu>
                  <MenuItem disable="true" mnemonicParsing="false" text="No Effects" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="View">
            <items>
                  <CheckMenuItem mnemonicParsing="false" text="Title" />
                  <RadioMenuItem mnemonicParsing="false" text="Binomial name">
                     <toggleGroup>
                        <ToggleGroup fx:id="check" />
                     </toggleGroup>
                  </RadioMenuItem>
                  <CheckMenuItem mnemonicParsing="false" text="Picture" />
                  <RadioMenuItem mnemonicParsing="false" text="Description" toggleGroup="$check" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TabPane layoutY="24.0" prefHeight="376.0" prefWidth="600.0" side="LEFT" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="24.0">
        <tabs>
          <Tab closable="false" disable="true" text="New">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab text="Rectangle">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane hmax="2.0" layoutX="186.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" vmax="2.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <Rectangle arcHeight="5.0" arcWidth="5.0" height="300.0" stroke="#bbe45b" strokeType="INSIDE" strokeWidth="4.0" width="300.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
